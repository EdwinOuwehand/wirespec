{
  "name": "standalone-vscode-ext",
  "displayName": "standalone-vscode-ext",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./build/extension",
  "dependencies": {
    "typescript": "^4.7.4",
    "vscode-languageclient": "^6.1.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.43.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2"
  },
  "scripts": {
    "compile": "tsc",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "clean:build": "npx --yes rimraf out",
    "clean:node_modules": "npx --yes rimraf node_modules",
    "start": "npm run compile"
  }
}
